<!DOCTYPE html>
<html>
  <head>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= tag :meta, name: :psj, action: action_name, controller: controller_name %>
    <%= csrf_meta_tags %>
  </head>
  <body>
  <!--Notifications-->
  <div id="jq-notificationWin" style="display: none" >
    <div id="jqmsg"  >
      Default text
    </div>
  </div>
  <%= content_tag :body, data: { action: action_name, controller: controller_name } do %>
  <div class="container">
    <div class="row">
      <div class="col-md-12">
        <div id='js-ajax-response'>
        </div>
        <%= render 'shared/navbar' %>
        <%= yield %>
        <%= insert_paloma_hook %>
      </div>
    </div>
  </div>
  </body>
</html>
<% end %>