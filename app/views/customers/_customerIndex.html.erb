<tbody>
<% @customers.each do |customer| %>
  <tr>
    <td><%= customer.id %></td>
    <td><%= customer.full_name %></td>
    <td><%= customer.phone %></td>
    <td><%= customer.email %></td>
    <td >
      <%= link_to ' ', items_customers_path(customer), class: 'fa fa-group icon' %>
    </td>
  </tr>
<% end %>
</tbody>